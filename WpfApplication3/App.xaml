<Application x:Class="WpfApplication3.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:viewModels="clr-namespace:WpfApplication3.ViewModels"
             xmlns:mvvm="clr-namespace:WpfApplication3.MVVM"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <viewModels:MainViewModel x:Key="MainViewModel" />

        <mvvm:InverseVisibilityConverter x:Key="InverseVisibilityConverter" />
        <mvvm:BooleanVisibilityConverter x:Key="BooleanVisibilityConverter" />
    </Application.Resources>
</Application>
